{
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "script"
  },
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "plugins": ["github", "jest"],
  "extends": ["plugin:github/recommended"],
  "rules": {
    "camelcase": "off",
    "filenames/match-regex": "off",
    "import/extensions": "off",
    "import/no-commonjs": "off",
    "i18n-text/no-en": "off"
  }
}
