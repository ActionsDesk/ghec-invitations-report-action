  "devDependencies": {
    "@github/prettier-config": "^0.0.6",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.6.3",
    "rimraf": "^5.0.1"
  },
  "prettier": "@github/prettier-config",
